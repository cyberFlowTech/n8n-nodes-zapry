{
	"name": "n8n-nodes-zapry",
	"version": "0.0.9",
	"description": "Zapry nodes for n8n ",
	"keywords": [
		"n8n-community-node-package",
		"zapry",
		"zapry-client",
		"zapry-client-node",
		"zapry-client-n8n",
		"zapry-client-n8n-node",
		"zapry-client-n8n-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "AI",
		"email": "ai@cyberflow.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cyberFlowTech/n8n-nodes-zapry.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"test": "jest"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ZapryApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Zapry/Zapry.node.js",
			"dist/nodes/Zapry/ZapryTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.20",
		"@types/luxon": "^3.7.1",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^24.0.13",
		"@types/sanitize-html": "^2.16.0",
		"@typescript-eslint/parser": "~8.36",
		"eslint": "^8.57.1",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^5.0.1",
		"jest": "^29.7.0",
		"jest-mock-extended": "^4.0.0",
		"n8n-workflow": "*",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.0",
		"typescript": "~5.8.3"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"mime-types": "^3.0.1",
		"zod": "3.24.0",
		"zod-to-json-schema": "^3.24.6"
	},
	"overrides": {
		"pkce-challenge": "3.0.0",
		"zod": "3.24.0"
	},
	"peerDependencies": {
		"luxon": "*",
		"n8n-workflow": "*",
		"sanitize-html": "*"
	}
